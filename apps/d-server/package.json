        "eslint-plugin-jest": "^27.2.1",
        "nodemon": "^2.0.22",
        "nx": "16.4.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "express": "^4.18.2",
        "socket.io": "^4.7.1"
    }
